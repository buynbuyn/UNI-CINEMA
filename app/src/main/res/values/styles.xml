<resources>
    <style name="RoundedPosterStyle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>


    </style>
    <style name="SeatStyle">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:background">@drawable/seat_background</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="NavDrawerText">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="CustomNavTextStyle">
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
    </style>
</resources>
